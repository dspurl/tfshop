<view class="content b-t"><block wx:for="{{addressList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['checkAddress',['$0'],[[['addressList','',index]]]]]]]}}" class="list b-b" bindtap="__e"><view class="wrapper"><view class="address-box"><block wx:if="{{item.defaults}}"><text class="tag">默认</text></block><text class="address">{{(item.location?item.location+'(':'')+item.address+" "+(item.house?')'+item.house:'')}}</text></view><view class="u-box"><text class="name">{{item.name}}</text><text class="mobile">{{item.cellphone}}</text></view></view><text data-event-opts="{{[['tap',[['addAddress',['edit','$0'],[[['addressList','',index]]]]]]]}}" class="yticon icon-bianji" catchtap="__e"></text><text data-event-opts="{{[['tap',[['deleteAddress',[index,'$0'],[[['addressList','',index]]]]]]]}}" class="cuIcon-delete" catchtap="__e"></text></view></block><block wx:if="{{$root.g0==0}}"><view><view class="no-data flex justify-center">还没有收货地址, 请添加~</view></view></block><button data-event-opts="{{[['tap',[['getWxaddAddress']]]]}}" class="add-btn1" bindtap="__e">获取收货地址</button><button data-event-opts="{{[['tap',[['addAddress',['add']]]]]}}" class="add-btn2" bindtap="__e">手动新增地址</button></view>