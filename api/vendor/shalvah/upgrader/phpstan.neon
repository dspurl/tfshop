parameters:
    level: 5
    excludePaths:
      - './src/PrettyPrinter/Standard.php'
    reportUnmatchedIgnoredErrors: false
    inferPrivatePropertyTypeFromConstructor: true
    ignoreErrors:
    - '#Method .+ should return .+\|null but return statement is missing.#'
