{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-2"
  ],
  "plugins": ["transform-vue-jsx", "transform-runtime", "transform-decorators-legacy", "transform-es2015-modules-commonjs"],
  "env": {
    "development":{
      "plugins": ["dynamic-import-node"]
    }
  }
}
